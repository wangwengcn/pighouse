<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pighouse.server.domain.User" table="USER">
		<id name="userId" type="java.lang.Long" column="ID">
			<generator class="increment"></generator>
		</id>
		<property name="email" type="string" column="USER_NAME"/>
		<property name="displayName" type="string" column="DISPLAY_NAME"/>
		<property name="password" type="string" column="PASSWORD"/>
	</class>
</hibernate-mapping> 