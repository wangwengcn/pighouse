<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pighouse.server.domain.TopicType" table="TOPIC_TYPE">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="string" column="NAME"/>
		<property name="descript" type="string" column="DESCRIPT"/>
		<property name="code" type="string" column="CODE"/>
		<property name="displayOrder" type="java.lang.Integer" column="DISPLAY_ORDER"/>
	</class>
</hibernate-mapping> 